<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    tools:ignore="ExtraText">
    app:startDestination="@id/exerciseListFragment">
    <fragment
        android:id="@+id/exerciseListFragment"
        android:name="ifgoiano.urt.fragmentosgymnavigationcomponent.ExerciseListFragment"
        android:label="Lista de Exercícios"
        tools:layout="@layout/fragment_exercise_list">
        <action
            android:id="@+id/action_exerciseListFragment_to_exerciseDetailFragment"
            app:destination="@id/exerciseDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/exerciseDetailFragment"
        android:name="ifgoiano.urt.fragmentosgymnavigationcomponent.ExerciseDetailFragment"
        android:label="Detalhes do Exercício"
        tools:layout="@layout/fragment_exercise_detail" />

    <argument android:name="exerciseName" app:argType="string" />
    <argument android:name="exerciseDescription" app:argType="string" />

</navigation>